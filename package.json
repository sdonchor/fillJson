{
  "name": "filljson",
  "displayName": "FillJSON",
  "description": "An extension that assists in filling out JSON files with key and string value pairs by automatically adding quotes, colons, and moving the cursor.",
  "version": "0.1.0",
  "publisher": "sdonchor",
  "engines": {
      "vscode": "^1.50.0"
  },
  "categories": [
      "Other", "Formatters", "Snippets"
  ],
  "main": "./extension.js",
  "contributes": {
      "commands": [
          {
              "command": "filljson.toggle",
              "title": "FillJSON: Toggle JSON assist"
          }
      ],
      "keybindings": [
          {
              "command": "filljson.nextField",
              "key": "tab",
              "when": "jsonAutoQuotesEnabled == true"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "postinstall": "node ./node_modules/vscode/bin/install",
      "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.50.0",
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.0",
      "@types/node": "^14.0.0",
      "typescript": "^4.0.3",
      "vscode-test": "^1.4.0"
  }
}
